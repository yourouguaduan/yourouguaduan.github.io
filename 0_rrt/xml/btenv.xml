<!-- copy from denso_btenv2-6.xml for using with AVPRRT_ORTH3 and a modified bottle -->
<Environment>
  <Robot file="densowithbase.xml"></Robot>
  
  <!-- <Kinbody file="base.xml"></Kinbody> -->

  <Kinbody name="floor">
    <Body type="static">
      <Geom type="box">
	<extents>2 2 0.05</extents>
	<Translation>0 0 -0.05</Translation>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box1">
    <!-- side wall -->
    <Body type="static">
      <Geom type="box">
	<extents>0.05 0.3 0.695</extents>
	<diffusecolor>1.00 0.2745 0.1608</diffusecolor>
      </Geom>
    </Body>
  </Kinbody>

  <Kinbody name="box2">
